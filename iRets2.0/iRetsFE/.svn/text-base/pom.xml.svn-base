  <build>
        <finalName>imls-fe</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>


			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>exec-maven-plugin</artifactId>
				<version>1.1</version>
			</plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>tomcat-maven-plugin</artifactId>
                <configuration>
                    <systemProperties>
                        <js.ext.debug>true</js.ext.debug>
                        <js.bc.debug>true</js.bc.debug>
                    </systemProperties>
                </configuration>
            </plugin>
			

			<plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>jaxb2-maven-plugin</artifactId>
                <version>1.3</version>
                <executions>          
                    <execution><id>model</id>
                        <goals>
                            <goal>xjc</goal>
                        </goals>     
		                <configuration>
		              		<schemaDirectory>src/main/resources/xsd</schemaDirectory>
		              		<packageName>com.irets.generated</packageName>		                       		
		       			</configuration>                     
		            </execution>		            
                  </executions>                                                  
            </plugin>



            <plugin>
                <groupId>mtgr8</groupId>
                <artifactId>framework</artifactId>
                <version>1.0-SNAPSHOT</version>
                <configuration>
                    <wsdlLocation>/users/developermtgr8/Downloads/work-wsdls/model.wsdl</wsdlLocation>
                    <templateLocationStr>/users/developermtgr8/Downloads/work-wsdls/templates</templateLocationStr>
                    <outDir>/users/developermtgr8/Downloads/work-wsdls/nFolder</outDir>
                    <fileTypeString>js</fileTypeString>
                    <templatesString>source.vm*source-enumeration.vm*source-simple.vm</templatesString>
                    <customClass>test</customClass>
                    <isGenerateSingleFile>true</isGenerateSingleFile>
                    <mappingFileName>/users/developermtgr8/Downloads/work-wsdls/mapping_sencha.properties</mappingFileName>
                </configuration>
            </plugin>

			<plugin>
				<groupId>org.mortbay.jetty</groupId>
				<artifactId>maven-jetty-plugin</artifactId>
				<configuration>
					<contextPath>/</contextPath>
					<scanIntervalSeconds>0</scanIntervalSeconds>
					<stopKey>foo</stopKey>
					<stopPort>9999</stopPort>
					
					<!--useTestClasspath>true</useTestClasspath-->
					<systemProperties>
						<systemProperty>
							<name>basedir</name>
							<value>${basedir}</value>
						</systemProperty>
                        <systemProperty>
                            <name>js.ext.debug</name>
                            <value>true</value>
                        </systemProperty>
                        <systemProperty>
                            <name>js.bc.debug</name>
                            <value>true</value>
                        </systemProperty>
                        <systemProperty>
                            <name>org.mortbay.jetty.Request.maxFormContentSize</name>
                            <value>10000000</value>
                        </systemProperty>
					</systemProperties>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-dependency-plugin</artifactId>
				<executions>
					 <execution>
						 <phase>process-resources</phase>
						 <goals>
							 <goal>copy-dependencies</goal>
						 </goals>
						 <configuration>
							 <includeArtifactIds>mysql-connector-java</includeArtifactIds>
						 </configuration>
					 </execution>
				 </executions>
			</plugin>
