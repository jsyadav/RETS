package com.irets.persistence;public class DaoUtil {    /**     * @param className the name of the class - returns "get" + className + "s"     * @return Returns the select query name.     */    static String getSelectQuery(String className) {        return "get" + className + "s";    }    /**     * @param className the name of the class - returns "get" + className     * @return Returns the find query name.     */    static String getFindQuery(String className) {        return "get" + className;    }    /**     * @param className the name of the class - returns "add" + className     * @return Returns the insert query name.     */    static String getInsertQuery(String className) {        return "insert" + className;    }    /**     * @param className the name of the class - returns "update" + className     * @return Returns the update query name.     */    static String getUpdateQuery(String className) {        return "update" + className;    }    /**     * @param className the name of the class - returns "delete" + className     * @return Returns the delete query name.     */    public static String getDeleteQuery(String className) {        return "delete" + className;    }}